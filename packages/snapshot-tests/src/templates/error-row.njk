{%- macro errorRow(test, resolveSnapshot) -%}
  <tr>
    <td colspan="3">
      {%- if test.hasSnapshot -%}
        <img src="{{ resolveSnapshot(test) }}" alt="Test Diff {{ test.snapshot }}"/>
      {%- else -%}
        <pre>{{ test.messages | join('.') }}</pre>
      {%- endif -%}
    </td>
  </tr>
{%- endmacro -%}
