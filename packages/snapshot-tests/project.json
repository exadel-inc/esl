{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx jest"
      },
      "dependsOn": [
        "^build"
      ]
    },
    "run:update": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx jest -u"
      },
      "dependsOn": [
        "^build"
      ]
    }
  }
}
