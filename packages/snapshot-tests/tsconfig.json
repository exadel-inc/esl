{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "alwaysStrict": true,
    "target": "ES6",
    "module": "ES2022",
    "moduleResolution": "Node",
  }
}
