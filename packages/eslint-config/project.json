{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "eslint-config",
  "targets": {
    "lint": {
      "executor": "nx:noop",
      "dependsOn": ["lint:eslint"]
    },
    "pack": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm pack --pack-destination=../../target/",
        "cwd": "packages/eslint-config"
      }
    }
  }
}
