{
  "name": "@exadel/eslint-plugin-esl",
  "private": false,
  "version": "5.3.2",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "esl"
  ],
  "publishConfig": {
    "access": "public",
    "scope": "@exadel"
  },
  "license": "MIT",
  "description": "Helper ESLint rules to find and migrate ESL (@exadel/esl) library deprecations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.{js,ts,md}",
    "CHANGELOG.md",
    "README.md"
