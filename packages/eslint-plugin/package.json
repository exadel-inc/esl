  "dependencies": {
    "kleur": "^4.1.5",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/semver": "^7.7.0"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0 || >=9.0.0"
  },
  "maintainers": [
    "ala-n <astsefanovich@exadel.com>",
    "yadamska <yadamska@exadel.com>",
    "dshovchko <dshovchko@exadel.com>",
    "abarmina <abarmina@exadel.com>",
    "alesun <alesun@exadel.com>"
  ]
