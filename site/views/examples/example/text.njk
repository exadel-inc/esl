      }
    </style>
    <h2><!-- text --></h2>
    <div class="text">
      Hello text
      <!-- paragraph x2 -->
    </div>
    <button type="button">
      <!-- text x5 -->
    </button>
  </script>
  <script type="text/plain" id="snippet-js-1">
    document.querySelector('form').onsubmit = (e) => {
      e.preventDefault();
      const json = JSON.stringify(Object.fromEntries(new FormData(e.target)));
      alert(`Form submitted with ${json}!`);
    };
  </script>
