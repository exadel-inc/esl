{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "bootstrap-sha": "5015e1a3fd8040bc25d8332ab0aeb6659459b3dd",
  "always-update": true,
  "plugins": [
    {
      "type": "node-workspace",
      "components": ["site", "e2e", "eslint-config", "eslint-plugin"],
      "updateAllPackages": true
    },
    {
      "type": "linked-versions",
      "components": ["eslint-config", "eslint-plugin"]
    }
  ],
  "prerelease": false,
  "include-v-in-tag": true,
  "pull-request-header": "chore: release ${version}",
  "group-pull-request-title-pattern": "chore: release ${version}",
  "packages": {
    ".": {
      "update-type": "node",
      "changelog-path": "CHANGELOG.md",
      "updateAllPackages": true
    },
    "eslint-config": {
      "component": "eslint-config",
      "update-type": "node"
    },
    "eslint-plugin": {
      "component": "eslint-plugin",
      "update-type": "node"
    }
  }
}
